{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "gen-db-types": "npx kysely-codegen --dialect=sqlite --url=./.wrangler/state/d1/DB.sqlite3 --out-file=./app/types/db-generated.d.ts",
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "wrangler": "export NO_D1_WARNING=true && wrangler pages dev ./public --binding $(cat .env) --port 3000 --local --persist --compatibility-date=2023-26-02"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.13.0",
    "@remix-run/cloudflare-pages": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.6",
    "kysely": "^0.23.4",
    "kysely-d1": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xstate": "^4.37.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "better-sqlite3": "^8.1.0",
    "eslint": "^8.27.0",
    "kysely-codegen": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.8.4",
    "wrangler": "^2.2.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
